{
    "manifest_version": 3,
    "name": "LinkedIn Highlight Search",
    "version": "1.0",
    "description": "Search LinkedIn using highlighted text",
    "permissions": ["contextMenus", "tabs"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_title": "LinkedIn Highlight Search"
    },
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
}